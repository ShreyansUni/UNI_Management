@model UNI_Management.ViewModel.EmployeeViewModel  

<script src="https://cdn.ckeditor.com/ckeditor5/41.4.2/classic/ckeditor.js"></script>

<!--begin::Content-->

<div class="d-flex flex-column flex-column-fluid" id="kt_content">

    <!--begin::Post-->
    <div class="post d-flex flex-column-fluid" id="kt_post">
        <!--begin::Container-->
        <div id="kt_content_container" class="container-xxl">
            <form id="employeeForm" asp-action="EmployeePost" asp-controller="Employee" method="post" enctype="multipart/form-data">

                <div class="row">
                    <div class="col-md-12">
                        <!--begin::Card-->
                        <div class="card">
                            <div class="row p-5">
                                <div class="col-md-6">
                                    <h1>Employee Details</h1>
                                </div>
                                <div class="col-md-6 text-end">

                                    <a class="btn btn-outline-primary rounded-2 border border-primary" onclick="history.back()">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left-short" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M12 8a.5.5 0 0 1-.5.5H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5a.5.5 0 0 1 .5.5" />
                                        </svg>Back
                                    </a>
                                </div>
                            </div>

                            <!--begin::Card body-->
                            <div class="card-body pt-5">
                                <div class="row">
                                    <div class="form-group">
                                        <input type="hidden" asp-for="employeeDetails.EmployeeId" class="form-control" />
                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.FirstName">FirstName</label>
                                        <input type="text" asp-for="employeeDetails.FirstName" class="form-control" />
                                        <span asp-validation-for="employeeDetails.FirstName" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.MiddleName">MiddleName</label>
                                        <input type="text" asp-for="employeeDetails.MiddleName" class="form-control" />
                                        <span asp-validation-for="employeeDetails.MiddleName" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.LastName">LastName</label>
                                        <input type="text" asp-for="employeeDetails.LastName" class="form-control" />
                                        <span asp-validation-for="employeeDetails.LastName" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="row pt-5">
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.ContactNumber1">ContactNumber</label>
                                        <input type="tel" asp-for="employeeDetails.ContactNumber1" class="form-control" />
                                        <span asp-validation-for="employeeDetails.ContactNumber1" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.ContactNumber2">AlternateNumber</label>
                                        <input type="tel" asp-for="employeeDetails.ContactNumber2" class="form-control" />
                                        <span asp-validation-for="employeeDetails.ContactNumber2" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-4">

                                        <label asp-for="employeeDetails.Email">Email</label>
                                        @if (Model.employeeDetails.Email == null)
                                        {
                                            <input type="email" asp-for="employeeDetails.Email" class="form-control" />
                                        }
                                        else
                                        {
                                            <input type="email" readonly asp-for="employeeDetails.Email" class="form-control" />
                                        }
                                        <span asp-validation-for="employeeDetails.Email" class="text-danger"></span>

                                    </div>
                                </div>
                                <div class="row pt-5">
                                    <div class="col-md-12">
                                        <label asp-for="employeeDetails.Address">Address</label>
                                        <input type="text" asp-for="employeeDetails.Address" class="form-control" />
                                        <span asp-validation-for="employeeDetails.Address" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="row pt-5">
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.Country">Country</label>
                                        <select class="form-select form-control" asp-for="employeeDetails.Country">
                                            <option value="">Select Country</option>
                                            <option value="India">India</option>
                                            <option value="USA">USA</option>
                                            <option value="UK">UK</option>
                                        </select>
                                        <span asp-validation-for="employeeDetails.Country" class="text-danger"></span>

                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.State">State</label>
                                        <select class="form-select form-control" asp-for="employeeDetails.State">
                                            <option value="">Select State</option>
                                            <option value="Gujrat">Gujrat</option>
                                            <option value="Rajsthan">Rajsthan</option>
                                            <option value="Maharastra">Maharastra</option>
                                        </select>
                                        <span asp-validation-for="employeeDetails.State" class="text-danger"></span>

                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.Birthdate">BirthDate</label>
                                        <input type="date" asp-for="employeeDetails.Birthdate" class="form-control" />
                                        <span asp-validation-for="employeeDetails.Birthdate" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="row pt-5">
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.Education">Education</label>
                                        <input type="text" asp-for="employeeDetails.Education" class="form-control" />
                                        <span asp-validation-for="employeeDetails.Education" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-4">
                                        <label asp-for="employeeDetails.EmployeeImage">EmployeeImage</label>
                                        <input type="file" asp-for="employeeDetails.EmployeeImage" class="form-control" />
                                        <span asp-validation-for="employeeDetails.EmployeeImage" class="text-danger"></span>
                                    </div>

                                     <div class="col-md-4">
                                        @{
                                            var filePath = Model.employeeDetails.Photo;
                                            bool isPdf = false;
                                            if (Model != null && !string.IsNullOrEmpty(filePath))
                                            {
                                                isPdf = filePath.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase);
                                            }
                                        }
                                        <div>
                                            @if (isPdf)
                                            {
                                                <img class="img-fluid border border-1 mx-2" style="height:150px;"
                                                     src="~/assets/logo/pdf-svgrepo-com.svg" />
                                            }
                                            else
                                            {
                                                <img class="img-fluid border border-1 mx-2" style="height:150px;"
                                                     src="~/Documents/Employee/@Model.employeeDetails.EmployeeId/@Model.employeeDetails.Photo"
                                                     alt="Image related to client" />
                                            }
                                        </div>
                                    </div>                                 
                                    @* <div class="col-md-4 pt-7">
                                        @if (Model != null && !string.IsNullOrEmpty(Model.employeeDetails.Photo))
